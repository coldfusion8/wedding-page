{
  "name": "template",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "start:m": "ng serve --host 0.0.0.0",
    "build": "ng build",
    "lint": "npm run eslint && npm run prettier:c && npm run stylelint",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "prettier:w": "prettier --write \"src/**/*.{js,ts}\"",
    "prettier:c": "prettier --check \"src/**/*.{js,ts}\"",
    "eslint": "eslint . --ext .ts src/ --max-warnings 0",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --karma-config karma.conf.js --watch=false",
    "test:watch": "ng test --karma-config karma.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.12",
    "@angular/cdk": "^17.1.3",
    "@angular/common": "^17.3.12",
